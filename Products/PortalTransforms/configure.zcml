<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    i18n_domain="Products.PortalTransforms"
    >

  <five:deprecatedManageAddDelete class=".TransformEngine.TransformTool" />

  <five:deprecatedManageAddDelete class=".Transform.Transform" />

  <genericsetup:registerProfile
      name="PortalTransforms"
      title="PortalTransforms"
      description="Extension profile for default PortalTransforms setup."
      provides="Products.GenericSetup.interfaces.EXTENSION"
      directory="profiles/default"
      />

</configure>
